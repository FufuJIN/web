<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5">
  <display-name>Welcome to Tomcat</display-name>
  <description>
     Welcome to Tomcat
  </description>  
   <servlet>
        <servlet-name>servletLogin</servlet-name>
        <servlet-class>servlet.servletLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletLogin</servlet-name>
        <url-pattern>/servletLogin</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>servletGetData</servlet-name>
        <servlet-class>servlet.servletGetData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletGetData</servlet-name>
        <url-pattern>/servletGetData</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>servletEcharts</servlet-name>
        <servlet-class>servlet.servletEcharts</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletEcharts</servlet-name>
        <url-pattern>/servletEcharts</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>servletSelectByDate</servlet-name>
        <servlet-class>servlet.servletSelectByDate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletSelectByDate</servlet-name>
        <url-pattern>/servletSelectByDate</url-pattern>
    </servlet-mapping>
    
   <welcome-file-list>  
        <welcome-file>Vhome.jsp</welcome-file>  
    </welcome-file-list>  
</web-app>